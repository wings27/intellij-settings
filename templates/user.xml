<templateSet group="user">
  <template name="ifdatacode" value="if (data.code != 0) {&#10;    window.Util.toast(&quot;$MSG$：code=&quot; + data.code + &quot;,&quot; + data.popups.content);&#10;    return;&#10;}$END$" toReformat="true" toShortenFQNames="true">
    <variable name="MSG" expression="&quot;操作失败&quot;" defaultValue="&quot;操作失败&quot;" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="res" value="@javax.annotation.Resource&#10;    private $TYPE$ $camelType$;&#10;    " toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="className()" defaultValue="" alwaysStopAt="true" />
    <variable name="camelType" expression="decapitalize(TYPE)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="todo" value="// todo @$USER$&#10;throw new java.lang.RuntimeException(&quot;todo&quot;);" description="java todo" toReformat="true" toShortenFQNames="true">
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
    </context>
  </template>
  <template name="post" value="$.post($URL$, {$PARAM$}, function (res) {&#10;    if (res.code != 0) {&#10;        window.Util.toast(&quot;操作失败：code=&quot; + res.code + &quot;. &quot; + res.popups.content);&#10;        }&#10;});&#10;" toReformat="true" toShortenFQNames="true">
    <variable name="URL" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
</templateSet>